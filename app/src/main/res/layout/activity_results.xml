<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="870dp"
    android:layout_height="330dp"
    android:background="@color/grey" >

    <TextView
        android:id="@+id/results"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:layout_centerHorizontal="true"
        android:background="@color/grey"
        android:text="Match Results"
        android:textColor="@color/blue"
        android:textSize="@dimen/f_text_30" />

    <LinearLayout
        android:id="@+id/a_head"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:layout_below="@+id/results"
        android:layout_marginStart="290dp"
        android:background="@color/white"
        android:orientation="horizontal" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20sp"
            android:layout_marginRight="20sp"
            android:background="@color/white"
            android:textColor="@color/red"
            android:textSize="@dimen/f_text_30"
            android:text="@string/set_1" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20sp"
            android:layout_marginRight="20sp"
            android:background="@color/white"
            android:textColor="@color/red"
            android:textSize="@dimen/f_text_30"
            android:text="@string/set_2" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20sp"
            android:layout_marginRight="20sp"
            android:background="@color/white"
            android:textColor="@color/red"
            android:textSize="@dimen/f_text_30"
            android:text="@string/set_3" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20sp"
            android:layout_marginRight="20sp"
            android:background="@color/white"
            android:textColor="@color/red"
            android:textSize="@dimen/f_text_30"
            android:text="@string/set_4" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20sp"
            android:layout_marginRight="20sp"
            android:background="@color/white"
            android:textColor="@color/red"
            android:textSize="@dimen/f_text_30"
            android:text="@string/set_5" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/a_home"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:layout_below="@+id/a_head"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="20dp"
        android:background="@color/white"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/name_h"
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20sp"
            android:layout_marginRight="20sp"
            android:background="@color/white"
            android:textColor="@color/red"
            android:textSize="@dimen/f_text_30" />

        <TextView
            android:id="@+id/set_1_h"
            android:layout_width="68dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20sp"
            android:layout_marginRight="20sp"
            android:background="@color/white"
            android:textColor="#000000"
            android:textSize="@dimen/f_text_30" />

        <TextView
            android:id="@+id/set_2_h"
            android:layout_width="68dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20sp"
            android:layout_marginRight="20sp"
            android:background="@color/white"
            android:textColor="#000000"
            android:textSize="@dimen/f_text_30" />

        <TextView
            android:id="@+id/set_3_h"
            android:layout_width="68dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20sp"
            android:layout_marginRight="20sp"
            android:background="@color/white"
            android:textColor="#000000"
            android:textSize="@dimen/f_text_30" />

        <TextView
            android:id="@+id/set_4_h"
            android:layout_width="68dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20sp"
            android:layout_marginRight="20sp"
            android:background="@color/white"
            android:textColor="#000000"
            android:textSize="@dimen/f_text_30" />

        <TextView
            android:id="@+id/set_5_h"
            android:layout_width="68dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20sp"
            android:layout_marginRight="20sp"
            android:background="@color/white"
            android:textColor="#000000"
            android:textSize="@dimen/f_text_30" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/a_visitor"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:layout_below="@+id/a_home"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="20dp"
        android:background="@color/white"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/name_v"
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20sp"
            android:layout_marginRight="20sp"
            android:background="@color/white"
            android:textColor="@color/red"
            android:textSize="@dimen/f_text_30" />

        <TextView
            android:id="@+id/set_1_v"
            android:layout_width="68dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20sp"
            android:layout_marginRight="20sp"
            android:background="@color/white"
            android:textColor="#000000"
            android:textSize="@dimen/f_text_30" />

        <TextView
            android:id="@+id/set_2_v"
            android:layout_width="68dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20sp"
            android:layout_marginRight="20sp"
            android:background="@color/white"
            android:textColor="#000000"
            android:textSize="@dimen/f_text_30" />

        <TextView
            android:id="@+id/set_3_v"
            android:layout_width="68dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20sp"
            android:layout_marginRight="20sp"
            android:background="@color/white"
            android:textColor="#000000"
            android:textSize="@dimen/f_text_30" />

        <TextView
            android:id="@+id/set_4_v"
            android:layout_width="68dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20sp"
            android:layout_marginRight="20sp"
            android:background="@color/white"
            android:textColor="#000000"
            android:textSize="@dimen/f_text_30" />

        <TextView
            android:id="@+id/set_5_v"
            android:layout_width="68dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20sp"
            android:layout_marginRight="20sp"
            android:background="@color/white"
            android:textColor="#000000"
            android:textSize="@dimen/f_text_30" />

    </LinearLayout>

    <Button
        android:id="@+id/b_ok"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/b_bott_size"
        android:layout_below="@+id/a_visitor"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/button1"
        android:onClick="onClick"
        android:text="@string/close"
        android:textSize="@dimen/f_text_15" />

</RelativeLayout>